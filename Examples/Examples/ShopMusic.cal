let p1 = repeat 2 (0 * 2 * 4 * 0' * 2' * 0' * 4 * 0') in
let p2 = repeat 4 (0 * 0, * 0,<) in
let p3 = . * . * . * . * . * . * 0' * 1' * 1'< * 3' * 4'<< * 3' in
let p4 = repeat 2 (p1 # p2 # (put effect = scale 0.35 in p3)) in
let p = repeat 2 (p4 * (-2 @@ p4) * p4) in

put layout = 2 2 1 2 2 2 1 in
put root = 0 12 -2 in
put time = 2 1 in
put duration = 250 in
put synthesizer = 0.25 0.29 740 20 30 in

p

